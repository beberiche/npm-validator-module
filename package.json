  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^20.4.5",
    "prettier": "^3.0.0",
    "rollup": "^3.27.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "tslint": "^6.1.3",
